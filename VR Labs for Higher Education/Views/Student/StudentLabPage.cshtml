@{
    ViewData["Title"] = "Lab Details";
    var labId = ViewBag.LabId as string;
}

<div class="lab-container">
    <br /><br />
    <h2>Lab Details for @labId</h2>
    <br />
    <!-- Lab details here -->
    <div class="lab-actions">
        <a href="@Url.Action("PlayLab", new { id = labId })" class="btn btn-success mr-2">Play</a>
        <a href="@Url.Action("LabTutorial", new { id = labId })" class="btn btn-success mr-2">Play</a>
    </div>
</div>